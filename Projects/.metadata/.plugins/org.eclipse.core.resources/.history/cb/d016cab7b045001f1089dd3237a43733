public class TestLine {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ClassPathXmlApplicationContext context = new ClassPathXmlApplicationContext("beans.xml");
	    Object obj = context.getBean("L");
	    Line team = (Line) obj;
